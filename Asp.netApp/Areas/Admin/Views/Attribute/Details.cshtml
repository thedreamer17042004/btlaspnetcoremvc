
@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
@inject IViewLocalizer _localizer;
    <div class="page-content">
        <div class="container-fluid">

            <!-- start page title -->
            <div class="row">
                <div class="col-12">
                    <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                        <h4 class="mb-sm-0">@_localizer["tablename"]</h4>

                        <div class="page-title-right">
                            <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">@_localizer["view"]</a></li>
                                <li class="breadcrumb-item active">@_localizer["attribute"]</li>
                            </ol>
                        </div>

                    </div>
                </div>
            </div>
            <!-- end page title -->
            @await Component.InvokeAsync("Attribute", new { viewType = "Detail" })


        </div>

    </div>
    <!-- container-fluid -->
    </div>
