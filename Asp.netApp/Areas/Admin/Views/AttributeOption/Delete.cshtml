
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@inject IViewLocalizer _localizer;
 <form asp-action="Delete" asp-controller="AttributeOption" method="post">
<div class="page-content">
    <div class="container-fluid">

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                    <h4 class="mb-sm-0">@_localizer["tablename"]</h4>

                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">@_localizer["delete"]</a></li>
                            <li class="breadcrumb-item active">@_localizer["attributeoption"]</li>
                        </ol>
                    </div>

                </div>
            </div>
        </div>
        <!-- end page title -->
        @await Component.InvokeAsync("AttributeOption", new { viewType = "Detail", btnShow = true })


    </div>

</div>
<!-- container-fluid -->
    </div>
    </form>